{
	"name": "fortune",
	"displayName": "Fortune",
	"description": "Shows a random fortune message on startup",
	"publisher": "madprops",
	"version": "1.1.0",
	"engines": {
		"vscode": "^1.30.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.show_fortune",
				"title": "Show Fortune"
			}
		]
	},
	"scripts": {
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.3.1",
		"vscode": "^1.1.28",
		"eslint": "^5.13.0",
		"@types/node": "^8.10.25",
		"@types/mocha": "^2.2.42"
	},
	"keywords": ["toy", "fun", "fortune", "cookie", "quotes"],
	"icon": "icon.jpg",
	"repository": {
		"type": "git",
		"url": "https://github.com/madprops/fortune.git"
	}
}
